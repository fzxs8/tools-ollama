{
  "$schema": "https://wails.io/schemas/config.v2.json",
  "name": "ollama",
  "outputfilename": "ollama",
  "frontend:install": "npm install",
  "frontend:build": "npm run build",
  "frontend:dev:watcher": "npm run dev",
  "frontend:dev:serverUrl": "auto",
  "author": {
    "name": "xiao.qiang",
    "email": "xiaoqiang.li2125@chancegiver.com"
  },
  "dev:command": "bash -c 'sleep 2 && dlv --listen=:40000 --headless=true --api-version=2 --accept-multiclient exec ./build/bin/ollama'",
  "build:go:gcflags": "-gcflags=all=-N -l"
}